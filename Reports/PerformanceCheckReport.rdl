    <DataSource Name="LimsConnection">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=DESKTOP5\SQLEXPRESS;Initial Catalog=Merge9FebDB</ConnectString>
        <Prompt>Specify a user name and password for data source LimsConnection:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>7220e8b8-c06e-40ba-9ef5-cb8f328412c6</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="CalibrationDS">
      <Query>
        <DataSourceName>LimsConnection</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
				
				CONVERT(VARCHAR(10),CompletionDate, 103)CompletionDate,
				CONVERT(VARCHAR(10),NextDate, 103)
				NextDate,
				J.ID AS InventoryBasicItemDetailsID,
				J.InventoryBasicItemDetailsNumber,
				K.ID AS InventoryBasicDetailsID,
				K.ItemID,
				K.GradeReceived,
				L.Name AS ItemName,
				L.CategoryID,
				M.CategoryName AS CategoryName,
				IMC.Capacity,
				IMC.UnitID,
	       			IU.Unit,
                                case when I.Frequency=1 then'Monthly'
				when I.Frequency=2 then'Quarterly'
				when I.Frequency=3 then'Half Yearly'
				else 'Anual' End as Frequency
			 FROM 
			 (
				Select 
	X.InventoryMaintainanceAndCalibrationScheduleID AS ScheduleID, 
	Y.ID AS CompletionDateID, 
	Y.ScheduleDate AS CompletionDate, 
	Z.ID AS NextDateID, 
	Z.ScheduleDate AS NextDate 
from
(select 
	InventoryMaintainanceAndCalibrationScheduleDates.InventoryMaintainanceAndCalibrationScheduleID
from 
	InventoryMaintainanceAndCalibrationScheduleDates 
inner join InventoryMaintainanceAndCalibrationSchedule 
	on InventoryMaintainanceAndCalibrationScheduleDates.InventoryMaintainanceAndCalibrationScheduleID = InventoryMaintainanceAndCalibrationSchedule.ID
where InventoryMaintainanceAndCalibrationSchedule.Type = 'PerFormance-Check'
group by InventoryMaintainanceAndCalibrationScheduleID) X


Left Join 
(
Select D.ID, E.ScheduleDate, E.InventoryMaintainanceAndCalibrationScheduleID
from 
(select  A.ID, A.ScheduleDate, A.InventoryMaintainanceAndCalibrationScheduleID from InventoryMaintainanceAndCalibrationScheduleDates A where  A.InventoryMaintainanceAndCalibrationScheduleID IN
(select distinct ID from InventoryMaintainanceAndCalibrationSchedule B where B.Type = 'PerFormance-Check') and 
A.CompletionStatus='Completed') D
Right Join 
(Select C.InventoryMaintainanceAndCalibrationScheduleID, MAX(C.ScheduleDate) AS ScheduleDate From
(select  A.ID, A.ScheduleDate, A.InventoryMaintainanceAndCalibrationScheduleID from InventoryMaintainanceAndCalibrationScheduleDates A where  A.InventoryMaintainanceAndCalibrationScheduleID IN
(select distinct ID from InventoryMaintainanceAndCalibrationSchedule B where B.Type = 'PerFormance-Check') and 
A.CompletionStatus='Completed') C
group by C.InventoryMaintainanceAndCalibrationScheduleID) E ON D.InventoryMaintainanceAndCalibrationScheduleID = E.InventoryMaintainanceAndCalibrationScheduleID and
D.ScheduleDate = E.ScheduleDate
) Y ON X.InventoryMaintainanceAndCalibrationScheduleID = Y.InventoryMaintainanceAndCalibrationScheduleID


Left Join
(
Select D.ID, E.ScheduleDate, E.InventoryMaintainanceAndCalibrationScheduleID
from 
(select  A.ID, A.ScheduleDate, A.InventoryMaintainanceAndCalibrationScheduleID from InventoryMaintainanceAndCalibrationScheduleDates A where  A.InventoryMaintainanceAndCalibrationScheduleID IN
(select distinct ID from InventoryMaintainanceAndCalibrationSchedule B where B.Type = 'PerFormance-Check') and 
(A.CompletionStatus='In Progress' or A.CompletionStatus is null)) D
Right Join 
(Select C.InventoryMaintainanceAndCalibrationScheduleID, MIN(C.ScheduleDate) AS ScheduleDate From
(select  A.ID, A.ScheduleDate, A.InventoryMaintainanceAndCalibrationScheduleID from InventoryMaintainanceAndCalibrationScheduleDates A where  A.InventoryMaintainanceAndCalibrationScheduleID IN
(select distinct ID from InventoryMaintainanceAndCalibrationSchedule B where B.Type = 'PerFormance-Check') and 
(A.CompletionStatus='In Progress' or A.CompletionStatus is null)) C
group by C.InventoryMaintainanceAndCalibrationScheduleID) E ON D.InventoryMaintainanceAndCalibrationScheduleID = E.InventoryMaintainanceAndCalibrationScheduleID and
D.ScheduleDate = E.ScheduleDate
) Z ON X.InventoryMaintainanceAndCalibrationScheduleID = Z.InventoryMaintainanceAndCalibrationScheduleID

			) B
			LEFT JOIN 
				dbo.InventoryMaintainanceAndCalibrationSchedule I ON B.ScheduleID = I.ID
			LEFT JOIN 
				dbo.InventoryBasicItemDetails J ON I.InventoryBasicItemDetailsID = J.ID
			LEFT JOIN 
				dbo.InventoryBasicDetails K ON J.InventoryBasicDetailsID = K.ID
			LEFT JOIN 
				dbo.InventoryItemMaster L ON K.ItemID = L.ID
			LEFT JOIN 
				dbo.InventoryCategoryMaster M ON L.CategoryID = M.ID
				LEFT JOIN 
				dbo.InventoryItemMaster IMC ON K.ItemID = IMC.ID 
				LEFT JOIN 
				dbo.UnitMasterInventory IU ON IMC.UnitID = IU.UnitId 
			
			WHERE
		B.CompletionDate BETWEEN @FromDate AND  @ToDate
				--L.InventoryTypeID = 2 AND
				--(L.ID = @iItemID OR @iItemID = 0)  AND
				--(J.InventoryBasicItemDetailsNumber = @sInventoryBasicItemDetailsNumber OR @sInventoryBasicItemDetailsNumber IS NULL)
			ORDER BY 
				B.CompletionDate DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
