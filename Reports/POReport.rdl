  <DataSets>
    <DataSet Name="DSPO">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PurchaseMasterID">
            <Value>=Parameters!PurchaseMasterID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  
  PurchaseRequestDetails.ItemMasterID
  ,PurchaseRequestDetails.Brand
  ,PurchaseRequestDetails.Grade
  ,PurchaseRequestDetails.PackSize
  ,PurchaseRequestDetails.Quantity
  
  ,PurchaseRequestDetails.Priority
  ,PurchaseRequestDetails.Specification
  ,PurchaseRequestDetails.Purpose
  ,PurchaseRequestDetails.Remark
  ,PurchaseRequestDetails.EstimatedLagTime
  ,PurchaseRequestDetails.Rate
  ,PurchaseRequestDetails.TickIf
  ,PurchaseRequestDetails.[COA/COC]
  ,PurchaseRequestDetails.PurchaseSupplierID AS [PurchaseRequestDetails PurchaseSupplierID]
  ,PurchaseRequestDetails.Comment
  ,PurchaseRequestDetails.Approvests
  ,PurchaseRequestDetails.CurrentStatus
  ,PurchaseRequestDetails.PONumber
  
  ,PurchaseSupplierDetails.SupplierName
  ,PurchaseSupplierDetails.SupplierContactPerson
  ,PurchaseSupplierDetails.SupplierContactNo
  ,PurchaseSupplierDetails.SupplierAddress
  ,PurchaseSupplierDetails.Product
 
 
  ,InventoryItemMaster.Name
  ,InventoryItemMaster.Code
  ,InventoryItemMaster.UnitID
  ,InventoryItemMaster.MinimumStock
 
FROM
  PurchaseRequestDetails
  INNER JOIN InventoryItemMaster
    ON PurchaseRequestDetails.ItemMasterID = InventoryItemMaster.ID
  INNER JOIN PurchaseSupplierDetails
    ON PurchaseRequestDetails.PurchaseSupplierID = PurchaseSupplierDetails.PurchaseSupplierID
Where PurchaseRequestDetails.PurchaseMasterID =@PurchaseMasterID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
