  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT SC.SampleCollectionId, PM.DisciplineId, SM.StatusName,
SC.SampleNo,sc.EnquirySampleID, PGM.ProductGroupId,
PGM.ProductGroupName , SGM.SubGroupName,MM.MatrixName FROM [LIMS].[Lims].SampleCollection SC
INNER JOIN  [LIMS].[Lims].EnquirySampleDetail ESD ON SC.EnquirySampleID=ESD.EnquirySampleID
INNER JOIN  [LIMS].[Lims].EnquiryDetail ED ON ED.EnquiryDetailId=ESD.EnquiryDetailId
INNER JOIN [LIMS].[Master].ProductGroupMaster PGM ON PGM.ProductGroupId=ED.ProductGroupId
INNER JOIN [LIMS].[Master].SubGroupMaster SGM ON SGM.SubGroupId=ED.SubGroupId
INNER JOIN [LIMS].[Master].MatrixMaster MM ON MM.MatrixId=ED.MatrixId
INNER JOIN [LIMS].[Lims].ParameterGroupMaster PM ON PM.ProductGroupId=PGM.ProductGroupId
AND pm.SubGroupId=sgm.SubGroupId and pm.ProductGroupId=pgm.ProductGroupId
 INNER JOIN  [LIMS].[Master].[StatusMaster] SM ON SM.StatusId=SC.StatusId
where  pm.DisciplineId = 1 AND sc.IsActive=0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
