@model IEnumerable<LIMS_DEMO.Areas.Arrival.Models.SampleArrivalModel>

@{
    ViewBag.Title = "SampleReceiverList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="main-container">
    <div class="pd-ltr-20 xs-pd-20-10">
        <div class="min-height-200px">
            <div class="page-header">
                <div class="row">
                    <div class="col-md-6 col-sm-12">
                        <div class="title">
                            <h4>Arrival</h4>
                        </div>
                        <nav aria-label="breadcrumb" role="navigation">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="index.html">Sample Receiver</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Sample Planner</li>
                                <li class="breadcrumb-item active" aria-current="page">Sample Planner List</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
            <!-- Default Basic Forms Start -->
            <div class="pd-20 card-box mb-30">
                <div class="clearfix">
                    <div class="pull-left">
                        <h4 class="text-blue h4">Assign Planner List</h4>
                    </div>
                </div>
                <hr />
                <table id="dtHorizontalExample" class="table table stripe hover nowrap">
                    <thead>
                        <tr class="thead-dark">
                            <th class="table-plus datatable-nosort">
                                SrNo.
                            </th>
                            <th>
                                Sample Type
                            </th>
                            <th>
                                Sample Location
                            </th>
                            <th>
                                Customer Name
                            </th>
                            <th>
                                Sample ID
                            </th>
                            <th>
                                ARC Report
                            </th>
                            <th>
                                Report Number
                            </th>
                            <th>
                                Status
                            </th>
                            <th>
                                Acknowledgement Report
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.SerialNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SampleTypeProductName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SampleLocation)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CustomerName)
                            </td>
                            @*<td>
         @Html.DisplayFor(modelItem => item.SampleName)
        </td>*@
                            <td>
                                @*@Html.ActionLink("View Details", "AddSampleArrival", new { SampleCollectionId = item.SampleCollectionId, SampleNo = item.SampleNo, ARCId = item.ARCId }, new { @class = "btn btn-sm btn-App fa fa-pencil" })
                                *@
                                <a href="#" onclick="funView(@item.SampleCollectionId, '@item.SampleNo',@item.WorkOrderID,@item.SampleTypeProductId,@item.EnquirySampleID);" class="sampleName" style="color:blue" data-toggle="modal" data-target=".bd-example-modal-lg">
                                    @Html.DisplayFor(modelItem => item.SampleName)
                                    @*@Html.DisplayFor(modelItem => item.SampleNo)*@
                                </a>
                            </td>
                            <td>
                                @if (item.Url.ToString().Trim() == "Ambient Air")
                                {
                                    <select class="form-control" id="@string.Concat("SelectShift",item)" name="@string.Concat("SelectShift",item)">
                                        <option value="Select Shift" disabled>Select Shift</option>
                                        <option value="Shift1">24 Hours</option>
                                        <option value="Shift2">8 Hours</option>
                                    </select>
                                    <a href="" class="btn btn-primary alignleft" data-toggle="tooltip" data-placement="top" onclick="GetShiftDataArc(@item,@item.SampleCollectionId,'@item.Url');">Print</a>
                                    //<a target="_blank" href="/Reporting/frmShowReport.aspx?path=AmbientAirARCReport.rdl&SampleCollectionId=@Model[i].SampleCollectionId" class="btn btn-primary alignleft" data-toggle="tooltip" data-placement="top" title="">Print</a>
                                }
                                else if (item.Url.ToString().Trim() == "Ambient Noise Level" || item.Url.ToString().Trim() == "Workplace Noise Level" || item.Url.ToString().Trim() == "Source Noise Level")
                                {
                                    <a>NA</a>
                                }
                                else
                                {
                                    <a target="_blank" href="/Reporting/frmShowReport.aspx?path=AnalysisReport.rdl&SampleCollectionId=@item.SampleCollectionId" class="btn btn-info alignleft" data-toggle="tooltip" data-placement="top" title="">Print</a>
                                }
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RequestNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CurrentStatus)
                            </td>
                            <td>
                                <a target="_blank" href="/Reporting/frmShowReport.aspx?path=AcknowledgementReport.rdl&SampleCollectionId=@item.SampleCollectionId&WorkOrderId=@item.WorkOrderID" class="btn btn-info alignleft" data-toggle="tooltip" data-placement="top" title="">Print</a>
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
            <!-- Default Basic Forms End -->
        </div>
    </div>
</div>
<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Assign Planer</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="planassg">

            </div>

        </div>
    </div>
</div>
<script src="~/Scripts/jquery.validate-vsdoc.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
@Scripts.Render("~/bundles/jqueryval")
@Scripts.Render("~/bundles/unobtrusive")
<script>
    $(document).ready(function () {
        $('#dtHorizontalExample').DataTable({
            "scrollX": true
        });
        $('.dataTables_length').addClass('bs-select');
    });
    function funView(SampleCollectionId, SampleNo, WorkOrderID,SampleTypeProductId,EnquirySampleID,CustomerName,CityName) {
        debugger;
        $("#planassg").empty();
        $("#planassg").load(`/Enquiry/TRF/_PlannerSelect?SampleCollectionId=${SampleCollectionId}&WorkOrderID=${WorkOrderID}&SampleNo=${SampleNo}&SampleTypeProductId=${SampleTypeProductId}&EnquirySampleID=${EnquirySampleID}`);
       
    }
    function PlannerSelectionSuccess(result) {
        debugger;
        if (result.status == "success") { alert("Planner Assign successfully") };
        window.location.reload();
    }
</script>


